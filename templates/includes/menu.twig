<div id="menu">
    <div class="list-group">
        {% for item in menu %}
            {% if item.page is same as 'Logout' %}
               <button class="list-group-item list-group-item-action" data-toggle="modal" data-target="#admin-logout"><i class="fas fa-{{ item.icon }} fa-2x"></i>Logout</button>
            {% else %}
                <a href="{{ item.url }}" class="list-group-item list-group-item-action"><i class="fas fa-{{ item.icon }} fa-2x"></i>{{ item.page }}</a>
            {% endif %}
        {% endfor %}
    </div>
</div>
